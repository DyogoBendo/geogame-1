{
    "name": "geogame",
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.hbs",
            "use": "@vercel/hbs"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}